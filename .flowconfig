[ignore]

[include]
modules

[libs]
flow-typed

[lints]
all=warn
unclear-type=off
untyped-type-import=error
untyped-import=off

[options]
emoji=true
exact_by_default=true
experimental.const_params=true
include_warnings=true
max_header_tokens=1
module.use_strict=true
module.file_ext=.js
module.file_ext=.mjs
module.file_ext=.json
module.system.node.resolve_dirname=node_modules
module.name_mapper='^eslint-config-none23-([a-z]+)$' -> 'modules/\1'
traces=2


[strict]
